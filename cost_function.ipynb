   "metadata": {},
   "outputs": [],
   "source": [
    "def cost_function(x,y):\n",
    "    y_predicted = 2*x-1\n",
    "    m = len(x)\n",
    "    j = 1/(2*m)*np.sum(np.dot(y_predicted-y,y_predicted-y))\n",
    "    return j"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
